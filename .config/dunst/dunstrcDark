[urgency_low]
    # IMPORTANT: colors have to be defined in quotation marks.
    # Otherwise the "#" and following would be interpreted as a comment.
    background = "#181818"
    foreground = "#d8d8d8"
    timeout = 10

[urgency_normal]
    background = "#181818"
    foreground = "#d8d8d8"
    timeout = 10

[urgency_critical]
    background = "#181818"
    foreground = "#d8d8d8"
    timeout = 0
