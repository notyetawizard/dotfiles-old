[urgency_low]
    # IMPORTANT: colors have to be defined in quotation marks.
    # Otherwise the "#" and following would be interpreted as a comment.
    background = "#f8f8f8"
    foreground = "#383838"
    timeout = 10

[urgency_normal]
    background = "#f8f8f8"
    foreground = "#383838"
    timeout = 10

[urgency_critical]
    background = "#f8f8f8"
    foreground = "#383838"
    timeout = 0
